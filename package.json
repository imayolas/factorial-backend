{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@apla/clickhouse": "^1.6.4",
    "@imayolas/clickhouse-migrate": "https://github.com/imayolas/clickhouse-migrate#1.1.1",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@types/knex": "^0.16.1",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "husky": "^7.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "dev:tsc": "tsc --watch",
    "dev:ingestor": "nodemon ./dist/ingestor/app/ingestor-entrypoint.js",
    "dev:api": "nodemon ./dist/api/app/api-entrypoint.js",
    "dev": "concurrently yarn:dev:tsc yarn:dev:ingestor yarn:dev:api",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register dist/**/*.test.js",
    "test:watch": "nodemon --watch dist --exec \"yarn test\"",
    "prettier": "prettier --config .prettierrc '**/*.{js,ts,json}' --write",
    "prepare": "husky install",
    "prepareClickhouse": "docker exec -it clickhouse clickhouse-client --query 'CREATE TABLE IF NOT EXISTS migrations (id Int32, data String, created_at DateTime) ENGINE = TinyLog()'"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "knex": "^1.0.1"
  }
}
